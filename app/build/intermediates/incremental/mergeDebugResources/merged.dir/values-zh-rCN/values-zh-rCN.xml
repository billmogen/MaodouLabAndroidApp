&lt;title>%s&lt;/title>
&lt;style type=\"text/css\">
body{margin:0;padding:0}
.inputstyle {
    -webkit-tap-highlight-color:rgba(255,255,255,0);
    width:100%%;
    height:44px;
    color:#000;
    background:0;
    border:0;
    font-size:16px;
    outline: none;
    -webkit-appearance:none
}

.btn {
    width:90%%;
    height:44px;
    line-height:44px;
    background:#A0C035;
    border:0;
    border-radius:4px;
    color:#fff;
    font-size:16px;
    text-align:center;
    margin-top:15px;
    display:block
}
.btn:disabled {
    background: #dddddd;
}
.error {
    color:red;
}
&lt;/style>
&lt;/head>

&lt;body>
&lt;script type=\"text/javascript\">
function login(form){
    var u=form[\"u\"].value,p=form[\"p\"].value;
    if(!u || !p)
        return false;
    form[\"s\"].disabled = true;
    return true;
}
&lt;/script>
&lt;form style=\"margin-top:45px;\" action=\"%s\" metho=\"get\" onsubmit=\"return login(this);\">
%s
